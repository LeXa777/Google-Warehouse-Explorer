* {
	font-family: Verdana, sans-serif;
	padding: 0px;
	margin: 0px;
}
div#main {
	position: absolute;
	width: 100%;
	height: 100%;
	overflow: hidden;
}
div#rotation_controls {
	position: absolute;
	z-index: 9999;
	visibility: hidden;
	background-image: url("blank.gif");
}
div#rotation_controls img {
	position: absolute;
	cursor: pointer;
}
var {
	visibility: hidden;
	position: absolute;
	top: 0px;
	left: 0px;
}
div#display {
	position: absolute;
	left: 0px;
	top: 0px;
	z-index: 999;
}
div#display div {
	position: absolute;
	display: table-cell;
	visibility: hidden;
	background-color: #ccccff;
	border: 1px solid #000000;
	opacity: 0.9;
}
div#display div img {
	width: 100%;
	height: 75%;
}
div#display div h1 {
	font-size: 100%;
	margin: 10px;
}
div#display div p {
	margin: 10px;
}
div#library {
	position: absolute;
	top: 5%;
	left: 1%;
	width: 98%;
	height: 93%;
	border: 1px solid #000000;
	overflow-y: scroll;
	overflow-x: hidden;
	text-align: right;
	z-index: 1;
}
div#library div {
	position: absolute;
}
div#library div img {
	cursor: move;
	position: absolute;
	bottom: 0px;
	right: 0px;
	border: 1px solid #000000;
}
div#library div object {
	position: absolute;
	top: 1px;
	left: 1px;
}
div#workspace {
	position: absolute;
	overflow: scroll;
	top: 5%;
	left: 1%;
	width: 85%;
	height: 93%;
	border: 1px solid #000000;
	z-index: 2;
}
div#highlight_n,
div#highlight_s,
div#highlight_e,
div#highlight_w {
	position: absolute;
	background-color: yellow;
	visibility: hidden;
	z-index: 999;
}
div#grid {
	position: absolute;
	top: 1px;
	left: 1px;
	background-color: #000000;
}
div.gridSquare {
	position: absolute;
	border: 1px solid #cccccc;
	float: left;
	background-color: #ffffff;
}
div#world {
	position: absolute;
	top: 2px;
	left: 2px;
}
div#world div.draggable {
	position: absolute;
	cursor: move;
}
div.button {
	display: table-cell;
	padding: 5px;
	border-color: #000000;
	border-width: 1px 2px 2px 1px;
	border-style: solid;
	background-color: #CCCCCC;
	cursor: pointer;
}
div.input {
	display: table-cell;
	border-color: #000000;
	border-width: 1px 2px 2px 1px;
	border-style: solid;
}
	
div#selector {
	position: absolute;
	top: 0px;
	right: 0px;
}
div#selector select,
div#selector input {
	width: 200px;
	margin: 0px 10px;
}
div#trash {
	position: absolute;
	width: 32px;
	height: 32px;
	border: 2px dashed #000000;
	padding: 7px;
	background-color: #ffffcc;
	z-index: 3;
	opacity: 0.7;
}
div#trash.highlight {
	border: 2px solid #ff0000;
	opacity: 0.8;
}
