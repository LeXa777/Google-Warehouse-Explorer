<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <context-param>
        <param-name>wonderland.art.url.base</param-name>
        <param-value>http://192.18.37.42/trunk/lg3d-wonderland-art/compiled_models</param-value>
    </context-param>
    <servlet>
        <servlet-name>FileUploadServlet</servlet-name>
        <servlet-class>org.jdesktop.wonderland.artupload.FileUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ArtUploadServlet</servlet-name>
        <servlet-class>org.jdesktop.wonderland.artupload.ArtUploadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DownloadServlet</servlet-name>
        <servlet-class>org.jdesktop.wonderland.artupload.DownloadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FileUploadServlet</servlet-name>
        <url-pattern>/fileservlet</url-pattern>
    </servlet-mapping>    
    <servlet-mapping>
        <servlet-name>ArtUploadServlet</servlet-name>
        <url-pattern>/artupload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DownloadServlet</servlet-name>
        <url-pattern>/art/*</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    </web-app>
