package org.jdesktop.wonderland.modules.videocube.client;

import java.awt.Image;
import java.util.Properties;
import org.jdesktop.wonderland.client.cell.registry.annotation.CellFactory;
import org.jdesktop.wonderland.client.cell.registry.spi.CellFactorySPI;
import org.jdesktop.wonderland.common.cell.state.CellServerState;
import org.jdesktop.wonderland.modules.videocube.common.VideoCubeServerState;

@CellFactory
public class VideoCubeCellFactory implements CellFactorySPI {

    public String[] getExtensions() {
        return new String[] {};
    }

    public <T extends CellServerState> T getDefaultCellServerState(Properties props) {
        VideoCubeServerState state = new VideoCubeServerState();
        return (T)state;
    }

    public String getDisplayName() {
        return "Video Cube";
    }

    public Image getPreviewImage() {
        return null;
   }
}
